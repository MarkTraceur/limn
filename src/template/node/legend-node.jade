fieldset
    legend(data-bind='visible: label, text: label')
    ul(data-bind="foreach: { data:entries, as:'entry' }")
        li.legend-entry(data-bind="attr: { id:entry.nodeId }, click: toggle")
            span.entry-label(data-bind='text: entry.label, style: { color:entry.color, opacity: node.disabled() ? 0.3 : 1 }')
            span.entry-value(data-bind='html: entry.value')
    

