{ Resource,
} = require './resource'
{ Model,
} = require './model'

/**
 * @class Root-class for all Models which sync with the server.
 * @extends Model
 * @mixes Resource
 */
class exports.StoredModel extends Model
    
    # Apply Resource mixin
    Resource.mix this
    
    
    /**
     * @constructor
     */
    -> super ...
    
    
    /**
     * Root prepended to all resource requests
     * @type {String|Function}
     */
    urlRoot : ->
        require('limn').mount()
    

