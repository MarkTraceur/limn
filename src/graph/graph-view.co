{ View,
} = require '../base/view'
{ Graph,
} = require './graph-model'

class exports.GraphView extends View
    template: 'graph'
    
    (idOrTab, action) ->
        @model = ko.observable()
        if idOrTab instanceof Graph
            @model idOrTab
        else
            @model new Graph id: idOrTab
            @model().fetch()
