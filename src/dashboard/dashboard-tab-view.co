{ View,
} = require '../base/view'
{ GraphView,
} = require '../graph/graph-view'

class exports.DashboardTabView extends View
    template: 'dashboard-tab'
    
    (tab) ->
        @model = ko.observable tab
        @graphs = ko.observableArray()
        # FIXME: a common pattern is synchronizing two observable arrays, and this isn't clean:
        for graph of @model().graphs()
            graphView = new GraphView graph
            @graphs.push graphView
