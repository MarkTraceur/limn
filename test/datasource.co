DataSource = limn.DataSource
util = limn.util

/******** Async Computed ********/
module 'Async Computed', do
    setup: ->
    teardown: ->

asyncTest 'create two DataSources', ->
    sameAttributes = id: 'rc_edits_count.yaml'
    datasource1 = new DataSource sameAttributes
    datasource2 = new DataSource sameAttributes
    
    strictEqual datasource1.loading, datasource2.loading, 'they have the same promise to load'
    datasource2.loading.done ->
        notEqual(datasource2.data, null, 'second one loads')
        start()
