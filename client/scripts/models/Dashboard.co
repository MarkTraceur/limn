define ['models/Graph', 'Storage'], (GraphModel, Storage) ->
    class Dashboard
        (id) ->
            @id = id
            @graphs = ko.observableArray()
            
            json = Storage.loadDashboard id
            $.extend(true, this, ko.mapping.fromJSON json)
            
            for tab of @tabs()
                for id of tab.graph_ids()
                    @graphs.push new GraphModel(id)
