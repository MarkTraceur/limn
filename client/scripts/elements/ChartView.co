define ['elements/Base'], (BaseElement) ->
    class ChartView extends BaseElement
        initialize: ->
            self = this
            self.metrics = ko.observableArray()
            self.isChart = true
            self.xScale = ko.computed ->
                self.metrics().join ', '
        
        template: 'element/svg'
        rendered: (elements, self) ->
            self.el = $ elements[0]
